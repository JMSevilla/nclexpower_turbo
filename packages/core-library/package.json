{
    "name": "@repo/core-library",
    "version": "0.0.0",
    "private": true,
    "main": "./index.ts",
    "types": "./index.ts",
    "scripts": {
        "lint": "eslint \"**/*.ts*\"",
        "generate:component": "turbo gen react-component",
        "test": "is-ci \"test:coverage\" \"test:ci\"",
        "test:coverage": "node --expose-gc ./node_modules/.bin/jest --coverage --logHeapUsage -w=30% --silent --testResultsProcessor jest-sonar-reporter",
        "test:coverage:windows": "node --expose-gc ../../node_modules/jest/bin/jest.js --coverage --silent --testResultsProcessor jest-sonar-reporter",
        "test:watch": "jest --watch",
        "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
        "clear:jest": "jest --clearCache",
        "test:ci": "jest --ci",
        "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\""
    },
    "devDependencies": {
        "@repo/eslint-config": "*",
        "@repo/typescript-config": "*",
        "@turbo/gen": "^1.11.3",
        "@types/eslint": "^8.56.1",
        "@types/node": "^20.10.6",
        "@types/qs": "^6.9.14",
        "@types/react": "^18.2.46",
        "@types/react-dom": "^18.2.18",
        "axios": "^1.6.8",
        "eslint": "^8.56.0",
        "is-ci": "^3.0.1",
        "is-ci-cli": "^2.2.0",
        "audit-ci": "^6.6.1",
        "react": "^18.2.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.8",
        "@mui/icons-material": "^5.14.3",
        "@mui/material": "^5.14.5",
        "iron-session": "^6.3.1",
        "next": "^13.4.19",
        "qs": "^6.12.1",
        "query-string": "^7.0.1",
        "react-async-hook": "^4.0.0",
        "react-hook-form": "^7.51.5",
        "yup": "^1.3.3",
        "zod": "^3.22.2"
    }
}
